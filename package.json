{
    "name": "syncfusion-gitlab-management",
    "displayName": "syncfusion-gitlab-management",
    "description": "",
    "version": "0.0.1",
    "publisher": "mohan",
    "engines": {
        "vscode": "^1.30.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [{
            "command": "extension.sayHello",
            "title": "Hello World"
        }],
        "viewsContainers": {
            "activitybar": [{
                "id": "gitlab-management-activity-bar",
                "title": "Gitlab Management",
                "icon": "logo.png"
            }]
        },
        "views": {
            "gitlab-management-activity-bar": [{
                "id": "gitlab-management",
                "name": "Gitlab Management"
            }]
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.21",
        "tslint": "^5.8.0",
        "@types/node": "^8.10.25",
        "@types/mocha": "^2.2.42"
    },
    "dependencies": {
        "@types/node-fetch": "^2.1.4",
        "node-fetch": "^2.3.0"
    }
}
